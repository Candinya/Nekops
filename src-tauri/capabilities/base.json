{
  "identifier": "nekops",
  "description": "base permissions for Nekops",
  "context": "local",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "clipboard-manager:allow-write-text",
    "window:allow-create"
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows",
    "android",
    "iOS"
  ]
}
